{
  "routes": [
    {
      "route": "/api/*",
      "methods": ["POST"],
      "rewrite": "/api/BrainPathFunction/{route}"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "mimeTypes": {
    ".json": "text/json"
  }
} 